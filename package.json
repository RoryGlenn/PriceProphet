{
  "name": "price-prophet",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://roryglenn.github.io/PriceProphet",
  "description": "A modern financial forecasting game built with React",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "6.4.0",
    "@mui/material": "^5.14.18",
    "@react-financial-charts/annotations": "2.0.0",
    "@react-financial-charts/axes": "2.0.0",
    "@react-financial-charts/coordinates": "2.0.0",
    "@react-financial-charts/core": "2.0.0",
    "@react-financial-charts/indicators": "2.0.0",
    "@react-financial-charts/interactive": "2.0.0",
    "@react-financial-charts/scales": "2.0.0",
    "@react-financial-charts/series": "2.0.0",
    "@react-financial-charts/tooltip": "2.0.0",
    "@react-financial-charts/utils": "2.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.61",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "10.0.0",
    "d3-format": "3.1.0",
    "d3-time-format": "4.1.0",
    "data-forge": "^1.10.1",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "technicalindicators": "3.1.0",
    "typescript": "^4.9.5",
    "uuid": "11.0.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/d3-format": "3.0.4",
    "@types/d3-time-format": "4.0.3",
    "@types/jest": "29.5.14",
    "@types/luxon": "3.4.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "10.4.20",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jest": "27.5.1",
    "lint-staged": "15.3.0",
    "postcss": "8.5.1",
    "prettier": "^3.1.1",
    "punycode": "2.3.1",
    "tailwindcss": "3.4.17",
    "ts-jest": "29.2.5",
    "typedoc": "^0.25.7"
  },
  "scripts": {
    "prepare": "husky",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci --passWithNoTests",
    "eject": "react-scripts eject",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "docs": "typedoc --out docs src",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm run test:ci"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@typescript-eslint/recommended",
      "plugin:react-hooks/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/react-app-env.d.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(d3-.*|@react-financial-charts)/).*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
